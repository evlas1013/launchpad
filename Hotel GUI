        jPanel_namePanel.setVisible(false);

    }

    /**
     *
     */
    public void updateDisplayInfo() {
        jLabel_hotelName.setText(myHotel.getName());
    }

    private void updateTable() {
        DefaultTableModel model = new DefaultTableModel();
        model.setRowCount(0); //delete all rows
        Object[] colNames = new Object[4];

        colNames[0] = "Guest";
        colNames[1] = "Owner";
        colNames[2] = "Check In Date";
        colNames[3] = "Room Number";
        model.setColumnIdentifiers(colNames);

        String name, owner;
        Date checkIn;
        int roomNum;
        for (int i = 0; i < myHotel.getGuestList().size(); i++) {
            Guest thisGuest = myHotel.getGuestList().get(i);
            name = thisGuest.getName();
            owner = thisGuest.getOwner();
            checkIn = thisGuest.getCheckIn();
            roomNum = thisGuest.getRoomNumber();
            Object[] guestList = new Object[4];
            guestList[0] = name;
            guestList[1] = owner;
            guestList[2] = checkIn;
            guestList[3] = roomNum;
            model.addRow(guestList);

        }
        model.setRowCount(myHotel.getCapacity()); //pad to capacity with empty rows
        jTable_guests.setModel(model);
    }

