    private void jButton1_setDateMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton1_setDateMouseClicked
        int todayMonth = 1;
        int todayDay = 1;
        int todayYear = 1970;
        try {
            todayMonth = Integer.parseInt(jTextField_todayMonth.getText());
            todayDay = Integer.parseInt(jTextField_todayDay.getText());
            todayYear = Integer.parseInt(jTextField_todayYear.getText());
        } catch (IllegalArgumentException ex) {
        }
        today = new Date(todayMonth, todayDay, todayYear);
        hideAllPanels();
        jPanel_namePanel.setVisible(true);
        jPanel_fields.setVisible(true);
        jPanel_foodOrder.setVisible(true);
        jPanel_table.setVisible(true);

        jLabel_todayDate.setText(today.toString());
        jTextField_todayDay.setVisible(false);
        jTextField_todayMonth.setVisible(false);
        jTextField_todayYear.setVisible(false);
        jLabel_todayMonthDiv.setVisible(false);
        jLabel_todayYearDiv.setVisible(false);
        jButton1_setDate.setVisible(false);
    }//GEN-LAST:event_jButton1_setDateMouseClicked

