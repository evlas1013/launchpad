    private void jButton_useTodayOutMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton_useTodayOutMouseClicked
        jTextField_checkOutDay.setText(Integer.toString(today.getDay()));
        jTextField_checkOutMonth.setText(Integer.toString(today.getMonth()));
        jTextField_checkOutYear.setText(Integer.toString(today.getYear()));
    }//GEN-LAST:event_jButton_useTodayOutMouseClicked

    private void jButton_checkOutMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton_checkOutMouseClicked
        int outD = Integer.parseInt(jTextField_checkOutDay.getText());
        int outM = Integer.parseInt(jTextField_checkOutMonth.getText());
        int outY = Integer.parseInt(jTextField_checkOutYear.getText());
        Date checkOut = new Date(outM, outD, outY);
        double lengthOfStay = (Math.floor(myHotel.findGuest(jTextField_ownerName.getText()).getCheckIn().difference(checkOut)));
        double amountOwed = lengthOfStay * myHotel.getRates();
        jTextField_lengthOfStay.setText(Double.toString(lengthOfStay));
        String amountOwedString = Double.toString(amountOwed);
        System.out.println(amountOwedString);
        jLabel_amountOwed.setText("$" + amountOwedString);
        jPanel_guestOwes.setVisible(true);
    }//GEN-LAST:event_jButton_checkOutMouseClicked

    private void jButton_completeCheckOutMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton_completeCheckOutMouseClicked
        Guest findGuest = myHotel.findGuest(jTextField_ownerName.getText());
        int i = 0;
        while (i < myHotel.getGuestList().size() && !myHotel.getGuestList().get(i).getOwner().equalsIgnoreCase(findGuest.getOwner())) {
            i++;
        }
        myHotel.removeGuest(i);
        jPanel_guestOwes.setVisible(false);
        updateTable();
    }//GEN-LAST:event_jButton_completeCheckOutMouseClicked

    private void jButton_saveMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton_saveMouseClicked
        File saveData = DataIO.setPath("Save As:");
        try {
            DataIO.writeFile(saveData, myHotel.saveData());
        } catch (IOException writeError) {
            JOptionPane.showMessageDialog(this, "Error Writing File");
        }// TODO add your handling code here:
    }//GEN-LAST:event_jButton_saveMouseClicked

    private void jButton_updateTableMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton_updateTableMouseClicked
        updateTable();        // TODO add your handling code here:
    }//GEN-LAST:event_jButton_updateTableMouseClicked

    private void jButton_submitDetailsActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton_submitDetailsActionPerformed
        myHotel.setHotelName(jTextField_hotelName.getText());
        myHotel.setCapacity(Integer.parseInt(jTextField_capacity.getText()));
        myHotel.setRate(Double.parseDouble(jTextField_rates.getText()));
        updateDisplayInfo();
        hideAllPanels();
        jPanel_namePanel.setVisible(true);
        jPanel_fields.setVisible(true);
        jPanel_foodOrder.setVisible(true);
        jPanel_table.setVisible(true);
    }//GEN-LAST:event_jButton_submitDetailsActionPerformed

    private void jButton_detailsActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton_detailsActionPerformed
        hideAllPanels();
        jTextField_hotelName.setText(myHotel.getName());
        jTextField_rates.setText(myHotel.getRates().toString());
        jTextField_capacity.setText(Integer.toString(myHotel.getCapacity()));
        jPanel_hotelDetails.setVisible(true);
    }//GEN-LAST:event_jButton_detailsActionPerformed

