    private void jButton_checkInMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton_checkInMouseClicked
        String guestName = jTextField_guestName.getText();
        String ownerName = jTextField_ownerName.getText();
        double weight = Double.parseDouble(jTextField_weight.getText());
        int birthdayMonth = Integer.parseInt(jTextField_birthdayMonth.getText());
        int birthdayDay = Integer.parseInt(jTextField_birthdayDay.getText());
        int birthdayYear = Integer.parseInt(jTextField_birthdayYear.getText());
        Date birthday = new Date(birthdayMonth, birthdayDay, birthdayYear);
        double age = birthday.difference(today);
        String breed = jTextField_breed.getText();
        int emptyRoom = myHotel.findEmptyRoom();
        int checkIn = myHotel.checkInGuest(today, birthday, guestName, ownerName,
                weight, age, emptyRoom, breed);
        if (checkIn == -1) {
            JOptionPane.showMessageDialog(null, "Cannot add guest. Hotel full.");
        } else {
            JOptionPane.showMessageDialog(null, "Guest Added.");
        }

        updateTable();

    }//GEN-LAST:event_jButton_checkInMouseClicked

