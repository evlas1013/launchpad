    private void jButton_useTodayInMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton_useTodayInMouseClicked
        jTextField_checkInDay.setText(Integer.toString(today.getDay()));
        jTextField_checkInMonth.setText(Integer.toString(today.getMonth()));
        jTextField_checkInYear.setText(Integer.toString(today.getYear()));
    }//GEN-LAST:event_jButton_useTodayInMouseClicked

    private void jButton_findGuestMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton_findGuestMouseClicked
        Guest findGuest = myHotel.findGuest(jTextField_ownerName.getText());
        jTextField_roomNumber.setText(Integer.toString(findGuest.getRoomNumber()));
        jTextField_guestName.setText(findGuest.getName());
        jTextField_birthdayDay.setText(Integer.toString(findGuest.getBirthDate().getDay()));
        jTextField_birthdayMonth.setText(Integer.toString(findGuest.getBirthDate().getMonth()));
        jTextField_birthdayYear.setText(Integer.toString(findGuest.getBirthDate().getYear()));
        jTextField_weight.setText(Double.toString(findGuest.getWeight()));
        jTextField_breed.setText(findGuest.getBreed());

        jTextField_checkInDay.setText(Integer.toString(findGuest.getCheckIn().getDay()));
        jTextField_checkInMonth.setText(Integer.toString(findGuest.getCheckIn().getMonth()));
        jTextField_checkInYear.setText(Integer.toString(findGuest.getCheckIn().getYear()));
        jLabel_foodOrder.setText(findGuest.getAmountOfFood() + " oz. of "
                + findGuest.getTypeOfFood() + " food.");

    }//GEN-LAST:event_jButton_findGuestMouseClicked

    private void jButton_openFileMouseClicked(java.awt.event.MouseEvent evt) {//GEN-FIRST:event_jButton_openFileMouseClicked
        myHotel.overWriteGuestList(DataIO.readGuestFile("guestList.txt", myHotel));
        updateTable();
        updateDisplayInfo();
    }//GEN-LAST:event_jButton_openFileMouseClicked

